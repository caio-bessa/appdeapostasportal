{
  "kind": "collectionType",
  "collectionName": "apps",
  "info": {
    "singularName": "app",
    "pluralName": "apps",
    "displayName": "App",
    "description": "Aplicativos de apostas esportivas"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "rating": {
      "type": "decimal",
      "min": 0,
      "max": 5
    },
    "pros": {
      "type": "text"
    },
    "cons": {
      "type": "text"
    },
    "bonus_info": {
      "type": "text"
    },
    "minimum_deposit": {
      "type": "decimal",
      "min": 0
    },
    "payment_methods": {
      "type": "text"
    },
    "license_info": {
      "type": "text"
    },
    "download_url": {
      "type": "string"
    },
    "logo_url": {
      "type": "string"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "inversedBy": "related_apps"
    }
  }
}